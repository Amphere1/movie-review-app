{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "SECRET_KEY": "@secret_key",
        "TMDB_API_KEY": "@tmdb_api_key"
    }
}
